.toplevelcss
  width 100%
  height 100%
  position relative
  z-index 300
  display flex
  flex-direction column
  &>.containercss
    width 100%
    flex 1
    box-sizing border-box
    border-bottom .1rem solid #f5f5f5
    overflow-y auto
    &>p
      height .4rem
      line-height .3rem
      font-size .15rem
      color #777
      font-weight bold
      box-sizing border-box
      border-top .1rem solid #f5f5f5
      padding-left .15rem
      text-align left
      position relative
      background-color #fff
      &>i
        position absolute
        top .05rem
        left 0
        width .05rem
        height .2rem
        background-color #7a79f1
    &>.maincss
      width 100%
      background-color #fff
      overflow hidden
      position relative
      &>span
        float left
        width 100%
        height .2rem
        color #9c4294
        font-size .14rem
        text-align right
        box-sizing border-box
        padding-right .2rem
    &>.subcss
      width 100%
      min-height .2rem
      background-color #fff
      overflow hidden
      position relative
      &>span
        float left
        width 100%
        height .2rem
        color #9c4294
        font-size .14rem
        text-align right
        box-sizing border-box
        padding-right .2rem
      &>.subitemcss
        clear both
        box-sizing border-box
        padding .1rem .1rem .2rem .1rem
        text-align left
        border-top .01rem solid #cacaca
        &>span
          float left
          width 100%
          height .2rem
          color #9c4294
          font-size .14rem
          text-align right
          box-sizing border-box
          padding-right .1rem
        &>.workingPeriod
          height .3rem
          font-size .15rem
          line-height .3rem
        &>.companyPosition
          height .3rem
          font-size .15rem
          line-height .3rem
          font-weight bold
        &>.jobDescription
          height 1rem
          font-size .14rem
          line-height .14rem
          background-color #f5f5f5
    .view_string
      width 100%
      display flex
      height .3rem
      line-height .3rem
      background-color #fff
      box-sizing border-box
      padding-left .1rem
      text-align left
      &>span
        width .7rem
        height .3rem
        font-size .14rem
        color: #777
        line-height .3rem
        margin-right .1rem
        box-sizing border-box
        padding-left .1rem
      &>:nth-child(2)
