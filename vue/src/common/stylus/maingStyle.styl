    &>p
      height .4rem
      line-height .3rem
      font-size .15rem
      color #777
      font-weight bold
      box-sizing border-box
      border-top .1rem solid #f5f5f5
      padding-left .15rem
      text-align left
      position relative
      background-color #fff
      &>i
        position absolute
        top .05rem
        left 0
        width .05rem
        height .2rem
        background-color #7a79f1
    &>.maincss
