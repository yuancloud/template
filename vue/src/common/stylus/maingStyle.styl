  &>.viewBigPicture
    width 100%
    height 100%
    background-color #000
    position relative
    z-index 300
    &>.pictureTabBar
      width 100%
      height .44rem
      background-color #fff
      line-height: .44rem
      box-sizing: border-box
      padding: 0 .2rem
      overflow hidden
      position relative
      &>.back
        font-size: .18rem
        position: absolute
        left: .2rem
        top: 0
      &>.getPicture
        position absolute
        right .2rem
        top 0
        font-size .18rem
        width .3rem
        text-align center
    &>.showImg
      position absolute
      top 50%
      left 50%
      margin-top -1.6rem
      margin-left -1.2rem
      width 2.4rem
      height 2.4rem
      &>img
        max-width 2.4rem
        height 2.4rem
      &>input
        position absolute
        right 0
        top 0
        width 2.4rem
        height 2.4rem
        opacity 0
